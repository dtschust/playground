{
  "name": "playground",
  "version": "1.0.0",
  "description": "A react playground",
  "main": "cli/cli.js",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --hot --inline --progress --colors --config webpack.config.js",
    "static": "NODE_ENV=production webpack --inline --progress --colors --config webpack.config.js",
    "serve-static": "npm run static && cd public && live-server",
    "test": "standard --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dtschust/playground.git"
  },
  "author": "Drew Schuster",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dtschust/playground/issues"
  },
  "homepage": "https://github.com/dtschust/playground#readme",
  "dependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "classnames": "^2.2.3",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.2",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.1",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "live-server": "^0.9.2",
    "react-transform-hmr": "^1.0.2",
    "standard": "^6.0.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
